{
    "manifest_version": 2,
    "name": "Bibo-1688 Import",
    "description": "Creates a Bibo product from a 1688 page.",
    "version": "0.2",
    "background": {
        "scripts": ["jquery-2.1.4.min.js", "background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "unicorn.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab", 
        "https://detail.1688.com/*"
    ]
}